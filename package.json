{
  "name": "snyk-docker-plugin",
  "description": "Snyk CLI docker plugin",
  "author": "snyk.io",
  "license": "Apache-2.0",
  "homepage": "https://github.com/snyk/snyk-docker-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-docker-plugin"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "lint": "prettier --check '**/*.ts' && tslint --format stylish '**/*.ts' --exclude 'node_modules/**'",
    "format": "prettier --loglevel warn --write '**/*.ts' && tslint --fix --format stylish '**/*.ts' --exclude 'node_modules/**'",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "tap test/system test/lib -R=spec --timeout=300",
    "prepare": "npm run build",
    "perf": "yarn run ts-node scripts/perf.ts"
  },
  "dependencies": {
    "@types/md5": "^2.1.33",
    "@types/minimatch": "^3.0.3",
    "@types/tar-stream": "^1.6.0",
    "@types/tmp": "^0.1.0",
    "debug": "^4.1.1",
    "dockerfile-ast": "0.0.16",
    "md5": "^2.2.1",
    "minimatch": "^3.0.4",
    "semver": "^6.1.0",
    "tar-stream": "^2.0.1",
    "tmp": "^0.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/node": "^8.10.48",
    "@types/sinon": "5.0.5",
    "@types/tar-fs": "^1.16.1",
    "prettier": "^1.17.1",
    "sinon": "^6",
    "tap": "12.3.0",
    "tar-fs": "^2.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
}
